				link: 'prices.html',
				classes: 'order-1 order-lg-1',
			},
			{
				name: 'Врачи',
				link: 'doctors.html',
				classes: 'order-2 order-lg-1',
			},
			{
				name: 'Информация',
				link: 'information.html',
				classes: 'order-3 order-lg-1',
			},
			{
				name: 'Вопросы/Ответы',
				link: 'Q&A.html',
				classes: 'order-4 order-lg-1',
			},
			{
				name: 'Отзывы',
				link: 'reviews.html',
				classes: 'order-lg-1',
			},
			{
				name: 'Вакансии',
				link: 'vacancies.html',
				classes: 'order-1 order-lg-1',
